# DriveShield Environment Configuration
# Copy this file to .env and update values as needed

# API Configuration
VITE_API_URL=http://localhost:8080/api
VITE_WS_URL=ws://localhost:8080/ws

# Set to 'false' to use real backend API instead of mock data
VITE_USE_MOCK=true

# Optional: ROS2 Bridge for real sensor data
# VITE_ROS_URL=ws://localhost:9090

# Optional: CARLA Simulator
# VITE_CARLA_HOST=localhost
# VITE_CARLA_PORT=2000
